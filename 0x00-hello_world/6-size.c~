int main(void)
{
	printf("Size of a char: %lu\n", sizeof(char));
	printf("Size of a char: %lu\n", sizeof(char));
	printf("Size of a char: %lu\n", sizeof(char));
	printf("Size of a char: %lu\n", sizeof(char));
	printf("Size of a float: %lu byte(s)\n", sizeof(float));
	return (0);
}
